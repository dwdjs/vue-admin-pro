{
  "name": "vue-admin-pro",
  "version": "2.0.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dll": "vue-cli-service dll",
    "commit": "npm run lint & npx git-cz",
    "styl": "npx stylelint ./src/**/*.{styl} --custom-syntax ./build/custom-syntax.js",
    "css": "npx stylelint ./src/**/*.{css,sass,scss} --fix",
    "deploy": "npm run build && npm run deployOnly",
    "deployOnly": "gh-pages -d dist --message $(git symbolic-ref --short -q HEAD)-$(git rev-parse --short HEAD)",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jskit/kit-admin.git"
  },
  "keywords": [
    "admin",
    "kit-admin",
    "jskit"
  ],
  "author": "cloudyan <1395093509@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jskit/kit-admin/issues"
  },
  "homepage": "https://github.com/jskit/kit-admin#readme",
  "dependencies": {
    "@dwdjs/utils": "^1.0.0",
    "axios": "0.19.0",
    "element-ui": "^2.12.0",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@tweenjs/tween.js": "^18.4.2",
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-e2e-cypress": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-plugin-pwa": "^3.12.1",
    "@vue/cli-plugin-unit-jest": "^4.0.5",
    "@vue/cli-service": "^3.12.1",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "cjs": "^0.0.11",
    "clipboard": "^2.0.4",
    "echarts": "^4.4.0",
    "eslint-plugin-import": "^2.18.2",
    "file-saver": "^2.0.2",
    "gh-pages": "^2.1.1",
    "js-cookie": "^2.2.1",
    "lint-staged": "^9.4.2",
    "lodash": "^4.17.15",
    "mockjs": "^1.1.0",
    "postcss-px-to-viewport": "^1.1.1",
    "postcss-pxtorem": "^4.0.1",
    "prettier": "^1.19.1",
    "qiniu-webpack-plugin": "^0.4.2",
    "qs": "^6.9.1",
    "register-service-worker": "^1.6.2",
    "screenfull": "^5.0.0",
    "script-loader": "^0.7.2",
    "sortablejs": "^1.10.1",
    "stylelint": "^11.1.1",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-scss": "^3.12.1",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "svg-sprite-loader": "^4.1.6",
    "svgo-loader": "^2.2.1",
    "vue-cli-plugin-dll": "^1.1.11",
    "vue-count-to": "^1.0.13",
    "vue-i18n": "^8.15.0",
    "vue-lazyload": "^1.3.3",
    "vue-markdown-loader": "2.4.1",
    "vue-multiselect": "^2.1.6",
    "vue-template-compiler": "^2.6.10",
    "xlsx": "^0.15.1"
  }
}
