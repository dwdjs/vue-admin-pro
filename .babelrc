{
  "presets": [
    "es2015",
    "stage-2",
    ["env", {
      "modules": false,
      "targets": {
        "safari": 9,
        "chrome": "56",
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"],
	"node": "current"
      }
    }]
  ],
  "comments": false,
  "plugins": [
    "add-module-exports",
    "syntax-dynamic-import",
    "transform-object-rest-spread",
    "transform-vue-jsx",
    "transform-runtime"
  ],
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": [ "istanbul" ]
    }
  }
}
